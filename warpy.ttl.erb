@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix atom:  <http://lv2plug.in/ns/ext/atom#> .
@prefix patch: <http://lv2plug.in/ns/ext/patch#> .
@prefix urid:  <http://lv2plug.in/ns/ext/urid#> .
@prefix param: <http://lv2plug.in/ns/ext/parameters#> .
@prefix midi:  <http://lv2plug.in/ns/ext/midi#> .
@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .

@prefix warpy: <https://milky.flowers/programs/warpy#> .

warpy:sample
	a lv2:Parameter ;
	rdfs:label "sample" ;
	rdfs:range atom:Path .

<% index = -1 %>
<https://milky.flowers/programs/warpy>
	a lv2:Plugin ;
	doap:name "Warpy" ;
	doap:license <https://www.gnu.org/licenses/gpl-3.0.en.html> ;
	lv2:project <https://milky.flowers/programs/warpy> ;
	lv2:requiredFeature urid:map ;
	lv2:optionalFeature lv2:hartRTCapable ;
	patch:writable warpy:sample ;
	lv2:port [
		a lv2:InputPort, atom:AtomPort ;
		atom:bufferType atom:Sequence ;
		atom:supports midi:MidiEvent,
			patch:Message ;
		lv2:designation lv2:control ;
		lv2:index <%= index += 1 %> ;
		lv2:symbol "in" ;
		lv2:name "In" ;
	] , [
		a lv2:OutputPort, lv2:AudioPort ;
		lv2:index <%= index += 1 %> ;
		lv2:symbol "out_l" ;
		lv2:name "Left" ;
	] , [
		a lv2:OutputPort, lv2:AudioPort ;
		lv2:index <%= index += 1 %> ;
		lv2:symbol "out_r" ;
		lv2:name "Right" ;
	] , [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index <%= index += 1 %> ;
		lv2:symbol "reverse" ;
		lv2:name "Reverse" ;
		lv2:portProperty lv2:toggled ;
		lv2:default 0.0 ;
		lv2:minimum 0.0 ;
		lv2:maximum 1.0 ;
	] , [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index <%= index += 1 %> ;
		lv2:symbol "speed_adjust" ;
		lv2:name "Speed Adjust" ;
		lv2:default 0.5 ;
		lv2:minimum 0.0 ;
		lv2:maximum 1.0 ;
	] , [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index <%= index += 1 %> ;
		lv2:symbol "speed_center" ;
		lv2:name "Speed Center" ;
		lv2:portProperty lv2:integer ;
		lv2:default 60.0 ;
		lv2:minimum 0.0 ;
		lv2:maximum 127.0 ;
	] , [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index <%= index += 1 %> ;
		lv2:symbol "speed_lower_scale" ;
		lv2:name "Lower Speed Scale" ;
		lv2:default -1.0 ;
		lv2:minimum -4.0 ;
		lv2:maximum 4.0 ;
	] , [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index <%= index += 1 %> ;
		lv2:symbol "speed_upper_scale" ;
		lv2:name "Upper Speed Scale" ;
		lv2:default 1.0 ;
		lv2:minimum -4.0 ;
		lv2:maximum 4.0 ;
	] , [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index <%= index += 1 %> ;
		lv2:symbol "pitch_adjust" ;
		lv2:name "Pitch Adjust" ;
		lv2:default 0.5 ;
		lv2:minimum 0.0 ;
		lv2:maximum 1.0 ;
	] , [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index <%= index += 1 %> ;
		lv2:symbol "pitch_center" ;
		lv2:name "Pitch Center" ;
		lv2:portProperty lv2:integer ;
		lv2:default 60.0 ;
		lv2:minimum 0.0 ;
		lv2:maximum 127.0 ;
	] , [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index <%= index += 1 %> ;
		lv2:symbol "pitch_lower_scale" ;
		lv2:name "Lower Pitch Scale" ;
		lv2:default -1.0 ;
		lv2:minimum -4.0 ;
		lv2:maximum 4.0 ;
	] , [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index <%= index += 1 %> ;
		lv2:symbol "pitch_upper_scale" ;
		lv2:name "Upper Pitch Scale" ;
		lv2:default 1.0 ;
		lv2:minimum -4.0 ;
		lv2:maximum 4.0 ;
	] , [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index <%= index += 1 %> ;
		lv2:symbol "attack_time" ;
		lv2:name "Attack Time" ;
		lv2:default 0.01 ;
		lv2:minimum 0.01 ;
		lv2:maximum 4.0 ;
	] , [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index <%= index += 1 %> ;
		lv2:symbol "attack_shape" ;
		lv2:name "Attack Shape" ;
		lv2:default 0.0 ;
		lv2:minimum -10.0 ;
		lv2:maximum 10.0 ;
	] , [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index <%= index += 1 %> ;
		lv2:symbol "decay_time" ;
		lv2:name "Decay Time" ;
		lv2:default 0.01 ;
		lv2:minimum 0.01 ;
		lv2:maximum 4.0 ;
	] , [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index <%= index += 1 %> ;
		lv2:symbol "decay_shape" ;
		lv2:name "Decay Shape" ;
		lv2:default 0.0 ;
		lv2:minimum -10.0 ;
		lv2:maximum 10.0 ;
	] , [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index <%= index += 1 %> ;
		lv2:symbol "sustain_level" ;
		lv2:name "Sustain Level" ;
		lv2:default 1.0 ;
		lv2:minimum 0.01 ;
		lv2:maximum 1.0 ;
	] , [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index <%= index += 1 %> ;
		lv2:symbol "release_time" ;
		lv2:name "Release Time" ;
		lv2:default 0.01 ;
		lv2:minimum 0.01 ;
		lv2:maximum 4.0 ;
	] , [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index <%= index += 1 %> ;
		lv2:symbol "release_shape" ;
		lv2:name "Release Shape" ;
		lv2:default 0.0 ;
		lv2:minimum -10.0 ;
		lv2:maximum 10.0 ;
	] , [
		a lv2:InputPort, lv2:ControlPort ;
		lv2:index <%= index += 1 %> ;
		lv2:symbol "gain" ;
		lv2:name "Gain" ;
		lv2:default 0.5 ;
		lv2:minimum 0.0 ;
		lv2:maximum 1.0 ;
	] .
